@{
    Layout = null;
}
<!doctype html>
<html>
<head>
    <title>Contacts Manager</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <h3>Information about contacts that have been changed during the given date range</h3>
    <p2 style="margin-left: 50px">Please specify the dates</p2>
    <br />
    <br />
    <form method="get" action="Contacts">
        <table style="margin-left: 30px">
            <tr>
                <td>Start Date </td>
                <td><input type="date" name="StartDate" required /></td>
            </tr>
            <tr height=20px></tr>
            <tr>
                <td>End Date </td>
                <td><input type="date" name="EndDate" required /></td>
            </tr>
            <tr height=20px></tr>
            <tr><td><input type="submit" value="Generate Excel File" style="width:150px; height:27px" /> </td></tr>
        </table>
    </form>
</body>
</html>